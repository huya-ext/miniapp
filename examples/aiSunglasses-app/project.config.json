{
  "name": "aiSunglasses-app",
  "outputPath": "dist",
  "releasePath": "release",
  "ignore": [
    "*.md",
    "LICENSE",
    ".git/**",
    ".gitignore",
    "package-lock.json"
  ],
  "builder": {
    "name": "@hyext/builder-beyond",
    "config": {
      "webSDKVersion": "1.10.0",
      "webTitle": "虎牙小程序",
      "useCssLoader": true,
      "designWidth": 750,
      "https": false,
      "host": "localhost",
      "port": 18089,
      "enableLint": false,
      "buildConfig": {
        "RN": [
          {
            "entry": "index_streamer.js",
            "extType": "zs_anchor_panel",
            "platform": "app"
          }
        ],
        "H5": [
          {
            "entry": "index_streamer.js",
            "extType": "pc_anchor_panel",
            "platform": "pc"
          }
        ]
      }
    }
  }
}
