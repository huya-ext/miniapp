{
  "name": "h5game",
  "outputPath": "hyext_dist",
  "releasePath": "hyext_release",
  "ignore": [
    "*.md",
    "LICENSE",
    ".git/**",
    ".gitignore",
    "package-lock.json",
    "node_modules/**"
  ],
  "builder": {
    "name": "@hyext/builder-h5game",
    "config": {
      "webTitle": "h5game",
      "h5Dir": "./dist",
      "convertEs6": false,
      "buildConfig": [
        {
          "extType": "pc_anchor_panel",
          "platform": "pc"
        },
        {
          "extType": "web_video_com",
          "platform": "web"
        },
        {
          "extType": "app_panel",
          "platform": "app"
        }
      ]
    }
  }
}
